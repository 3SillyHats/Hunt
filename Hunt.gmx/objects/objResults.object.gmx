        <arguments>
          <argument>
            <kind>1</kind>
            <string>for (i = 0; i &lt; PLAYERS_MAX; i++) {
    if not gamepad_is_connected(i) { continue; }
    var axis = gamepad_axis_value(i, gp_axislv);
    var up = axis &lt; -MENU_DEAD_ZONE or gamepad_button_check_pressed(i,gp_padu);
    var down = axis &gt; MENU_DEAD_ZONE or gamepad_button_check_pressed(i,gp_padd);
    
    if (gamepad_button_check_pressed(i, gp_face1)) {
        // Select option
        switch (selected) {
        case 0:
            audio_play_sound(sndBlip, 0, false);
            room_goto(global.selected_room);
            break;
        case 1:
            audio_play_sound(sndBlip, 0, false);
            room_goto(rmSelect);
            break;
        case 2:
            audio_play_sound(sndBlip, 0, false);
            room_goto(rmSetup);
            break;
        case 3:
            audio_play_sound(sndBlip, 0, false);
            game_end();
            break;
        }
    } else if (down) {
        // Move down
        if (not pressed[i]) {
            selected = (selected + 1) % array_length_1d(options);
            pressed[i] = true;
            audio_play_sound(sndBlip, 0, false);
        }
    } else if (up) {
        // Move up
        if (not pressed[i]) {
            selected = (selected + array_length_1d(options) - 1) % array_length_1d(options);
            pressed[i] = true;
            audio_play_sound(sndBlip, 0, false);
        }
    } else {
        pressed[i] = false;
    }
}

</string>
