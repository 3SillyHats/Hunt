        // Move down
        if (not pressed[i]) {
            selected = (selected + 1) % array_length_1d(options);
            pressed[i] = true;
        }
    } else if (axis &lt; -DEAD_ZONE) {
        // Move up
        if (not pressed[i]) {
            selected = (selected - 1) % array_length_1d(options);
            pressed[i] = true;
        }
    } else {
        pressed[i] = false;
    }
}

